@{
    ViewBag.Title = "Index";
}
   <!-- Carousel
    ================================================== -->
    <h2 class="img">
        <img src="../../Images/Carousel/ITintheD_logo.png" /> <span class="muted">It'll blow your mind.</span></h2>
    <div id="myCarousel" class="carousel slide span8 offset2">
      <div class="carousel-inner">
        <div class="item active">
          <img src="../../Images/Carousel/OutsourceDetroit2.jpg" alt="" />
          <div class="container">
            <div class="carousel-caption">
              @Html.ActionLink("Apply Now", "Register", "Account", null, new Dictionary<string, object> { {"class" , "btn btn-large btn-primary" }})
              @*@<a class="btn btn-large btn-primary" href="#">Sign up today</a>@*@
            </div>
          </div>
        </div>
        <div class="item">
          <img src="../../Images/Carousel/ITdGroup.jpg" alt="" />
          <div class="container">
            <div class="carousel-caption">
          
              <a class="btn btn-large btn-primary" href="#">Learn more</a>
            </div>
          </div>
        </div>
        <div class="item">
          <img src="../../Images/Carousel/Quicken.jpg" alt="" />
          <div class="container">
            <div class="carousel-caption">
          
              <a class="btn btn-large btn-primary" href="#">Browse gallery</a>
            </div>
          </div>
        </div>
      </div>
      <a class="left carousel-control" href="#myCarousel" data-slide="prev">&lsaquo;</a>
      <a class="right carousel-control" href="#myCarousel" data-slide="next">&rsaquo;</a>
    </div><!-- /.carousel -->
